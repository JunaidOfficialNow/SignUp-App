 <section id="menu" style="padding:0">
        <div class="logo">
            <h2>Admin Panel</h2>
        </div>
 
        <div class="items">
            <li><ion-icon name="pie-chart-outline"></ion-icon><a href="#">Dashboard</a></li>
            <li><ion-icon name="people-outline"></ion-icon><a href="#">Users</a></li>
            <li><ion-icon name="settings-outline"></ion-icon><a href="#">Settings</a></li>
          <li><ion-icon name="help-circle-outline"></ion-icon><a href="/admin/signout">Signout</a></li>
           
        </div>
    </section>
 
    <section id="interface" style="padding:0">
        <div class="navigation" style="z-index: 1;">
            <div class="n1">
                <div>
                    <ion-icon id="menu-btn" name="menu-outline"></ion-icon>
                </div>
                <div class="search">
                    <ion-icon name="search-outline"></ion-icon>
                    <form action="/admin/user-search/" method="get" id="search-form">

                    <input type="text" placeholder="Search" name="searchData" onchange="searchUser()" style="margin-bottom: 0;">
                    </form>
                </div>
            </div>
            <div class="profile">
                <ion-icon name="notifications-outline"></ion-icon>
                <ion-icon name="person-outline"></ion-icon>
                <h6>{{name}}</h6>
            </div>
        </div>
 
        <h3 class="page_name">Dashboard</h3>
 
        <div class="values">
          
            <div class="val-box" style="margin-top:30px;">
                <ion-icon name="add-circle-outline"></ion-icon>
                <div>
                    <h3><a href="/admin/create-user" class="fs-5 text-dark" style="text-decoration: none;">Add New User </a></h3>
                </div>
            </div>
           
         
         
        </div>
 
        <div class="board">
            <table width="100%" id="table">
                <thead>
                    <tr>
                        <td>username</td>
                        <td>email</td>
                        <td>status</td>
                        <td>Options</td>
                    </tr>
                </thead>
                <tbody>
                 {{#each users}}
                    <tr>
                        <td class="people">
                            <div class="people-de">
                                <h5>{{this.username}}</h5>
                            </div>
                        </td>
                        <td class="email">
                            <h5>{{this.email}}</h5>
                        </td>
                        <td class="active"><p>Active</p></td>
                        <td class="edit"><a href="/admin/edit-user/{{this._id}}" class="btn text-white btn-secondary">Edit</a> &nbsp;
                        <a href="/admin/delete-user/{{this._id}}" onclick="return confirm('Are you sure you want to delete {{this.username}}')" class="btn btn-danger text-white">Delete</a></td>
                    </tr>
                    {{/each}}
                  
                
                 
                </tbody>
            </table>
        </div>
    </section>
   <script>
      function searchUser(){
        document.getElementById('search-form').submit();
      }
   </script>